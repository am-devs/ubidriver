{
  "manifest_version": 3,
  "name": "Aprobador Odoo 18",
  "version": "1.0",
  "description": "Extensi√≥n para aprobar documentos en Odoo 18",
  "permissions": [
    "storage",
    "scripting",
    "cookies",
    "notifications",
    "tabs"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "ws://*/*",
    "wss://*/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Aprobador Odoo"
  },
  "background": {
    "service_worker": "background.js"
  }
}